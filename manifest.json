{
	"name": "metadata-error-report",
	"label": "Error Report Generator",
	"description": "A validation error report generator and resolver",
	"version": "1.0.0",
	"inputs": {
		"api-key": {
			"base": "api-key"
		}
	},
	"config": {
		"container_type": {
			"default": "all",
			"description": "A container type (singular) to accumulate, or all",
			"type": "string"
		},
		"file_type": {
			"default": "json",
			"description": "File Type of report (json or csv)",
			"type": "string"
		},
		"filename": {
			"default": "",
			"description": "Optional report name override",
			"type": "string"
		}
	},
	"environment": {
		"GPG_KEY": "0D96DF4D4110E5C43FBFB17F2D347EA6AA65421D",
		"LANG": "C.UTF-8",
		"PATH": "/usr/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
		"PYTHON_PIP_VERSION": "18.1",
		"PYTHON_VERSION": "3.7.1"
	},
	"command": "python run.py",
	"author": "Harsha Kethineni",
	"maintainer": "Harsha Kethineni",
	"cite": "List citations here.",
	"license": "Other",
	"source": "",
	"url": "",
	"custom": {
		"dicom-image": "flywheel/metadata-error-report-grp-2:0.0.1",
		"gear-builder": {
			"category": "analysis",
			"image": "flywheel/metadata-error-report"
		}
	}
